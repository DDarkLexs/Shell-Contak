DROP TABLE IF EXISTS empresa;
DROP TABLE IF EXISTS sessao_log;
DROP TABLE IF EXISTS funcionario;
DROP TABLE IF EXISTS notaMoeda;
DROP TABLE IF EXISTS moeda;


</sql><sql name="create">
CREATE TABLE empresa (
id_empresa INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
nome TEXT NOT NULL,
src TEXT,
nif TEXT,
email TEXT,
contato TEXT,
provincia TEXT,
municipio TEXT,
endereco TEXT);

CREATE TABLE moeda (
[id_moeda] INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
[$5] INTEGER,
[$10] INTEGER,
[$20] INTEGER,
[$50] INTEGER,
[$100] INTEGER,
[$200] INTEGER,
[$500] INTEGER,
[$1000] INTEGER,
[$2000] INTEGER,
[$5000] INTEGER);

CREATE TABLE notaMoeda (
[id_notaMoeda] INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
[nome] TEXT NOT NULL,
[vencimento] DATETIME NOT NULL,
[nota] TEXT,
[valido] BOOLEAN NOT NULL,
[datacad] DATETIME NOT NULL,
[id_moeda] INTEGER NOT NULL,
[id_usuario] INTEGER NOT NULL,
FOREIGN KEY([id_moeda]) REFERENCES [moeda]([id_moeda]),
FOREIGN KEY([id_usuario]) REFERENCES [usuario]([id_usuario]));


CREATE TABLE funcionario (
id_funcionario INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
nome TEXT NOT NULL UNIQUE,
src TEXT,
sexo TEXT NOT NULL,
nascimento DATE NOT NULL,
estadoCivil TEXT,
natural TEXT,
provincia_origem TEXT,
municipio TEXT,
residencia TEXT,
endereco TEXT,
telefone BLOB,
datacad DATETIME NOT NULL,
id_empresa INTEGER NOT NULL,
FOREIGN KEY(id_empresa) REFERENCES empresa(id_empresa));

CREATE TABLE [sessao_log] (
[id_sessao] INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
[data] DATETIME NOT NULL,
[id_usuario] INTEGER NOT NULL,
FOREIGN KEY([id_usuario]) REFERENCES [usuario]([id_usuario]));

CREATE TABLE [usuario] (
[id_usuario] INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
[senha] TEXT NOT NULL,
[role] TEXT NOT NULL,
[permissao] BOOLEAN NOT NULL,
[last_session] DATETIME,
[id_funcionario] INTEGER NOT NULL,
FOREIGN KEY([id_funcionario]) REFERENCES funcionario(id_funcionario));

</sql><sql name="event trigger functions">CREATE TRIGGER sessao_visto 
AFTER UPDATE OF last_session ON usuario
 BEGIN
	insert into sessao_log values (NULL, datetime('now'), new.id_usuario);
 END;</sql><current_tab id="0"/></tab_sql></sqlb_project>
